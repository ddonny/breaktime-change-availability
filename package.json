{
  "name": "moneychange",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "mocha": "^6.0.2",
    "react-scripts": "^2.1.8",
    "redux-mock-store": "^1.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.3",
    "proxyquire": "^2.0.1",
    "rc-table": "^6.3.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "reactstrap": "^6.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rewire": "^3.0.2",
    "sanitize.css": "^5.0.0",
    "styled-components": "^3.4.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "node scripts/cfg start",
    "start": "SET NODE_PATH=./src && start npm run watch-css && npm run start-js",
    "build-js": "SET NODE_PATH=./src node scripts/cfg build",
    "build": "npm-run-all build-css build-js",
    "old-test": "SET NODE_PATH=./src && react-scripts test --env=jsdom",
    "test": "SET NODE_PATH=./src && react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
